          </ids:propertySet>
          <ids:baseName>
            <ids:simpleValue>OccupancyNumber</ids:simpleValue>
          </ids:baseName>
        </ids:property>
        <ids:property cardinality="required">
          <ids:propertySet>
            <ids:simpleValue>Pset_SpaceOccupancyRequirements</ids:simpleValue>
          </ids:propertySet>
          <ids:baseName>
            <ids:simpleValue>MinimumHeadroom</ids:simpleValue>
          </ids:baseName>
        </ids:property>
        <ids:property cardinality="required">
          <ids:propertySet>
            <ids:simpleValue>Pset_SpaceOccupancyRequirements</ids:simpleValue>
          </ids:propertySet>
          <ids:baseName>
            <ids:simpleValue>IsOutlookDesirable</ids:simpleValue>
          </ids:baseName>
        </ids:property>
        <ids:property dataType="IFCTEXT" cardinality="required" instructions="UH-Romkoder">
          <ids:propertySet>
            <ids:simpleValue>NTNU_Reference</ids:simpleValue>
          </ids:propertySet>
          <ids:baseName>
