<ids:ids xmlns:xs="http://www.w3.org/2001/XMLSchema" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/1.0/ids.xsd" 
         xmlns:ids="http://standards.buildingsmart.org/IDS">
  <ids:info>
    <ids:title>UseCase2_06</ids:title>
  </ids:info>
  <ids:specifications>
    <ids:specification ifcVersion="IFC4X3_ADD2" name="The IfcDistributionBoards that are a Incomer, Feeder or Coupling must NEST one or more IfcDistributionPorts">
      <ids:applicability minOccurs="1" maxOccurs="unbounded">
        <ids:entity>
          <ids:name>
            <ids:simpleValue>IFCDISTRIBUTIONPORT</ids:simpleValue>
          </ids:name>
          <ids:predefinedType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="CABLE" />
              <xs:enumeration value="CABLECARRIER" />
              <xs:enumeration value="DUCT" />
            </xs:restriction>
          </ids:predefinedType>
        </ids:entity>
      </ids:applicability>
      <ids:requirements>
        <ids:partOf relation="IFCRELNESTS" cardinality="required">
          <ids:entity>
            <ids:name>
              <ids:simpleValue>IFCDISTRIBUTIONBOARD</ids:simpleValue>
            </ids:name>
            <ids:predefinedType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="Incomer" />
                <xs:enumeration value="Feeder" />
                <xs:enumeration value="Coupling" />
                <xs:enumeration value="Panel medium voltage switchgear" />
              </xs:restriction>
            </ids:predefinedType>
          </ids:entity>
        </ids:partOf>
      </ids:requirements>
    </ids:specification>
  </ids:specifications>
</ids:ids>