  </ids:info>
  <ids:specifications>
    <ids:specification ifcVersion="IFC2X3" name="binder" >
      <ids:applicability maxOccurs="unbounded">
        <!-- At least one facet -->
      </ids:applicability>
      <ids:requirements>
        <ids:property dataType="IFCLABEL">
          <ids:propertySet>
            <xs:restriction base="xs:string">
              <xs:pattern value="anas.*"/>
            </xs:restriction>
          </ids:propertySet>
          <ids:baseName>
            <ids:simpleValue>Anax</ids:simpleValue>
          </ids:baseName>
          <ids:value>
            <ids:simpleValue>yes</ids:simpleValue>
          </ids:value>
        </ids:property>
      </ids:requirements>
    </ids:specification>

    <ids:specification ifcVersion="IFC2X3" name="binder">
      <!-- Missing applicability -->
      <ids:requirements>
        <ids:property dataType="IFCTEXT">
          <ids:propertySet>
            <xs:restriction base="xs:string">
              <xs:pattern value="anas.*"/>
            </xs:restriction>
          </ids:propertySet>
          <ids:baseName>
            <ids:simpleValue>Anax</ids:simpleValue>
          </ids:baseName>
          <ids:value>
            <ids:simpleValue>yes</ids:simpleValue>
          </ids:value>
        </ids:property>
      </ids:requirements>
    </ids:specification>
  </ids:specifications>
