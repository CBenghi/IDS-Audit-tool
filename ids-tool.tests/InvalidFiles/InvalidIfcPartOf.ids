<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ids:ids xmlns:ids="http://standards.buildingsmart.org/IDS" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/ids_09.xsd">
	<ids:info>
		<ids:title>Example on use of attributes (for student research)</ids:title>
		<ids:copyright>buildingSMART International Ltd</ids:copyright>
		<ids:date>2022-01-12</ids:date>
	</ids:info>
	<ids:specifications>
		<!-- should fail because IfcRelNests cannot be child of an IFC2DCOMPOSITECURVE -->
		<ids:specification ifcVersion="IFC2X3" name="Aachen test" maxOccurs="unbounded">
			<ids:applicability>
				<ids:partOf relation="IfcRelNests">
					<ids:entity>
						<ids:name>
							<ids:simpleValue>IFC2DCOMPOSITECURVE</ids:simpleValue>
						</ids:name>
					</ids:entity>
				</ids:partOf>
			</ids:applicability>
			<ids:requirements>
				<ids:property maxOccurs="unbounded">
					<ids:propertySet>
						<ids:simpleValue>SomeValidPset</ids:simpleValue>
					</ids:propertySet>
					<ids:name>
						<ids:simpleValue>SomeValidName</ids:simpleValue>
					</ids:name>
				</ids:property>
			</ids:requirements>
		</ids:specification>
	</ids:specifications>
</ids:ids>
