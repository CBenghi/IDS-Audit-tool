			<ids:applicability maxOccurs="unbounded">
				<ids:entity>
					<ids:name>
						<ids:simpleValue>IFC2DCOMPOSITECURVE</ids:simpleValue>
					</ids:name>
				</ids:entity>
			</ids:applicability>
			<ids:requirements>
				<ids:partOf relation="IFCRELNESTS">
					<ids:entity>
						<ids:name>
							<ids:simpleValue>IFCWALL</ids:simpleValue>
						</ids:name>
					</ids:entity>
				</ids:partOf>
			</ids:requirements>
		</ids:specification>
	</ids:specifications>
