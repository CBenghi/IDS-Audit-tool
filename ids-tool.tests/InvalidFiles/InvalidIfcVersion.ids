	<ids:specifications>
		<!-- The ifcVersion string is not compatible with the schema, it will likely fail loading in automated implementations -->
		<ids:specification ifcVersion="IFC2X3 INVALIDIFCVERSION" name="binder">
			<ids:applicability maxOccurs="unbounded">
				<ids:entity>
					<ids:name>
						<xs:restriction base="xs:string">
							<xs:pattern value="IFCCOVERING"/>
						</xs:restriction>
					</ids:name>
					<ids:subType>
						<ids:simpleValue>CLADDING</ids:simpleValue>
					</ids:subType>
				</ids:entity>
			</ids:applicability>
			<ids:requirements>
				<ids:property dataType="IFCLABEL">
					<ids:propertySet>
						<xs:restriction>
							<xs:pattern value="anas.*"/>
						</xs:restriction>
					</ids:propertySet>
					<ids:name>
						<ids:simpleValue>Anax</ids:simpleValue>
					</ids:name>
					<ids:value>
						<ids:simpleValue>yes</ids:simpleValue>
					</ids:value>
				</ids:property>
			</ids:requirements>
		</ids:specification>
	</ids:specifications>
</ids:ids>
